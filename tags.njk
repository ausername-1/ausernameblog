---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
layout: layouts/basic.njk
eleventyComputed:
  title: Tagged “{{ tag }}”
permalink: /tags/{{ tag }}/
---
<h1 style="padding-left:20px;">Tagged &quot;{{ tag }}&quot;</h1>

{% set postslist = collections[ tag ] %}
{% include "postslist.njk" %}

<p style="padding-left:30px;">See <a href="{{ '/tags/' | url }}">all tags</a>.</p>
